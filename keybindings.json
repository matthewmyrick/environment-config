[
    {
        "key": "shift+cmd+g",
        "command": "runCommands",
        "args": {
            "commands": [
                "workbench.action.newGroupRight",
                "workbench.action.createTerminalEditor",
                {
                    "command": "workbench.action.terminal.sendSequence",

                    "args": {
                        "text": "gemini\n"
                    }
                }
            ],
        },
    },
    {
        "key": "shift+cmd+l",
        "command": "runCommands",
        "args": {
            "commands": [
                "workbench.action.newGroupRight",
                "workbench.action.createTerminalEditor",
                {
                    "command": "workbench.action.terminal.sendSequence",

                    "args": {
                        "text": "lazygit\n"
                    }
                }
            ],
        },
    },
    {
        "key": "shift+cmd+t",
        "command": "runCommands",
        "args": {
            "commands": [
                "workbench.action.newGroupRight",
                "workbench.action.createTerminalEditor"
            ],
        },
    }
